{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data":{
    "url": "https://raw.githubusercontent.com/Snitzel284/FIT3179/main/data/EltonJohnAlbumTracks.csv",
    "format": {"type": "csv"}
    },

  "width": 1000,
  "height": 300,
  "encoding": {
    "x": {"field": "Album", "type": "nominal", "axis": {"labelAngle": 90,"labelLimit": 300}},
    "y": {"field": "Danceability", "type": "quantitative", "scale": {"domain": [0, 1]}},
    "color": {
      "field": "Album",
      "type": "nominal",
      "scale": {
        "range": ["#fde0dd", "#fa9fb5", "#c51b8a", "#9b017d", "#7a0177", "#49006a", "#250045", "#000000"]
      },
      "legend": null
    }
  },

  "layer": [
    {
      "mark": {
        "type": "circle",
        "opacity": 0.8,
        "size": 30
      }
    },
    {
      "mark": {
        "type": "tick",
        "color": "black",
        "strokeWidth": 20,
        "thickness": 3,
        "size": 30
      },
      "encoding": {
        "y": {"aggregate": "mean", "field": "Danceability", "type": "quantitative"},
        "x": {"field": "Album", "type": "nominal"},
        "color": {"field": "Album", "type": "nominal"}
      }
    }
  ]
}
